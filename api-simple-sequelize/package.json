{
  "name": "api-simple-sequelize",
  "version": "1.0.0",
  "description": "sample api for uiTemplate",
  "main": "dist/app.js",
  "scripts": {
    "watch": "tsc -w -p ./",
    "start": "node bin/www",
    "postinstall": "npm run compile",
    "generate": "node dist/generate.js",
    "validate": "node dist/validate.js",
    "genUml": "node dist/genUml.js",
    "debug": "node --inspect-brk bin/www",
    "updateSchema": "node updateSchema.js",
    "compile": "./compile.sh 2> /dev/null",
    "clean": "./clean.sh",
    "collector": "cd src/graphql-gen/system/queries; graphql-document-collector '**/*.graphql' > ../../../../data/seed-queries.json; cd ../../../.. ; node dist/seeds/validate.js",
    "syncDb": "node dist/syncDb",
    "restoreData": "node dist/seeds/restoreDirect.js data/dump.json",
    "dumpData": "node dist/seeds/dumpDirect.js data/dump_1.json"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "apollo-cache-inmemory": "^1.2.4",
    "apollo-client": "^2.3.4",
    "apollo-link-batch-http": "^1.2.2",
    "apollo-upload-server": "^2.0.4",
    "body-parser": "^1.18.3",
    "compression": "^1.7.2",
    "cors": "^2.8.4",
    "dataloader": "^1.4.0",
    "dotenv": "^6.0.0",
    "graphql": "^0.13.2",
    "graphql-subscriptions": "^0.5.8",
    "graphql-tag": "^2.4.2",
    "lodash": "^4.17.10",
    "log4js": "^1.1.1",
    "oda-api-common": "^0.2.44",
    "oda-api-graphql": "^0.4.48",
    "oda-api-graphql-sequelize": "0.0.11",
    "oda-gen-common": "^0.1.21",
    "oda-gen-graphql": "^0.6.33",
    "oda-isomorfic": "^0.0.37",
    "oda-lodash": "^0.1.21",
    "sequelize": "^4.37.10",
    "sqlite3": "^4.0.0",
    "subscriptions-transport-ws": "^0.9.11"
  },
  "devDependencies": {
    "@types/fs-extra": "^4.0.0",
    "@types/graphql": "^0.12.4",
    "@types/inflected": "^1.1.29",
    "@types/mongoose": "^4.7.20",
    "@types/node": "^8.0.24",
    "@types/passport": "^0.4.3",
    "graphql-document-collector": "^0.2.1",
    "typescript": "^2.9.2"
  }
}
